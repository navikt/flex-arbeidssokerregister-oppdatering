<?xml version="1.0" encoding="UTF-8"?>
<configuration scan="false" debug="false">

	<appender name="Console" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>[%d, %-5p, %C:%L] - %msg %ex{5}%nopex%n</pattern>
		</encoder>
	</appender>

	<root level="INFO">
		<appender-ref ref="Console"/>
	</root>

</configuration>
